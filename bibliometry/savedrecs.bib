@article{ ISI:000323926500008,
Author = {Mueller, Andre and Wuensche, Hans-Joachim},
Title = {{Navigation Approaches for Mobile Robots - Past, Present and Possible
   Future Advances}},
Journal = {{AT-AUTOMATISIERUNGSTECHNIK}},
Year = {{2013}},
Volume = {{61}},
Number = {{4}},
Pages = {{290-300}},
Month = {{APR}},
Abstract = {{While robots, mobile or not, do not yet play an important role in
   everyday life, countless tasks that demand high precision and speed have
   been conquered by them. But their application domains get larger and
   larger, be it in form of autonomous robots able to discover deep sea
   regions, driver-assistance-systems in cars or unmanned drones in the
   sky. With every new application new requirements must be met, not only
   in terms of hardware like sensors or actors, but also with respect to
   perception- and navigation algorithms. Especially when harsh
   environments aggravate perception, robust navigation methods are
   required, that can cope with uncertain sensor measurements and select
   those robot motions out of many, which yield a safe and goal oriented
   behaviour. This paper attempts to showcase different navigation
   paradigms and their realizations on real mobile robots, as well as the
   reasons behind choosing a certain realization. The paper closes with an
   outlook towards possible future developments in the area of mobile robot
   navigation.}},
