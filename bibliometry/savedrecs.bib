@article{ ISI:000342282900067,
Author = {Jo, Kichun and Kim, Junsoo and Kim, Dongchul and Jang, Chulhoon and
   Sunwoo, Myoungho},
Title = {{Development of Autonomous Car-Part I: Distributed System Architecture
   and Development Process}},
Journal = {{IEEE TRANSACTIONS ON INDUSTRIAL ELECTRONICS}},
Year = {{2014}},
Volume = {{61}},
Number = {{12}},
Pages = {{7131-7140}},
Month = {{DEC}},
Abstract = {{An autonomous car is a self-driving vehicle that has the capability to
   perceive the surrounding environment and navigate itself without human
   intervention. For autonomous driving, complex autonomous driving
   algorithms, including perception, localization, planning, and control,
   are required with many heterogeneous sensors, actuators, and computers.
   To manage the complexity of the driving algorithms and the heterogeneity
   of the system components, this paper applies distributed system
   architecture to the autonomous driving system, and proposes a
   development process and a system platform for the distributed system of
   an autonomous car. The development process provides the guidelines to
   design and develop the distributed system of an autonomous vehicle. For
   the heterogeneous computing system of the distributed system, a system
   platform is presented, which provides a common development environment
   by minimizing the dependence between the software and the computing
