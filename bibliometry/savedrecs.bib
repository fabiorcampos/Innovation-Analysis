@article{ ISI:000333364300009,
Author = {Hautiere, Nicolas and Tarel, Jean-Philippe and Halmaoui, Houssam and
   Bremond, Roland and Aubert, Didier},
Title = {{Enhanced fog detection and free-space segmentation for car navigation}},
Journal = {{MACHINE VISION AND APPLICATIONS}},
Year = {{2014}},
Volume = {{25}},
Number = {{3, SI}},
Pages = {{667-679}},
Month = {{APR}},
Abstract = {{Free-space detection is a primary task for car navigation.
   Unfortunately, classical approaches have difficulties in adverse weather
   conditions, in particular in daytime fog. In this paper, a solution is
   proposed thanks to a contrast restoration approach on images grabbed by
   an in-vehicle camera. The proposed method improves the state-of-the-art
   in several ways. First, the segmentation of the fog region of interest
   is better segmented thanks to the computation of the shortest routes
   maps. Second, the fog density as well as the position of the horizon
   line is jointly computed. Then, the method restores the contrast of the
   road by only assuming that the road is flat and, at the same time,
   detects the vertical objects. Finally, a segmentation of the connected
   component in front of the vehicle gives the free-space area. An
   experimental validation was carried out to foresee the effectiveness of
   the method. Different results are shown on sample images extracted from
   video sequences acquired from an in-vehicle camera. The proposed method
   is complementary to existing free-space area detection methods relying
   on color segmentation and stereovision.}},
