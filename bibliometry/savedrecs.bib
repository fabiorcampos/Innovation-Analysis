@article{ ISI:000246192300019,
Author = {Malan, Stefano and Milanese, Mario and Borodani, Pandeli and Gallione,
   Alessandro},
Title = {{Lateral control of autonomous electric cars for relocation of public
   urban mobility fleet}},
