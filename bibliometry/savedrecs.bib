@article{ ISI:000245568600007,
Author = {Ozguner, Umit and Stiller, Christoph and Redmill, Keith},
Title = {{Systems for safety and autonomous behavior in cars: The DARPA Grand
   Challenge experience}},
Journal = {{PROCEEDINGS OF THE IEEE}},
Year = {{2007}},
Volume = {{95}},
Number = {{2}},
Pages = {{397-412}},
Month = {{FEB}},
Abstract = {{In this paper, we review technologies for autonomous ground vehicles and
   their present capabilities in research and in the automotive market. We
   outline technology requirements for enhanced functions and for
   infrastructure development. Since the recent Grand Challenge competition
   is a major force to advance technology in this field, we specifically
   refer to our experiences in developing a participating vehicle. We
   present a multisensor platform that has been proven in an off-road
   environment. It combines different sensing modalities that inherently
   yield uncertain information. Finite-state machines are formulated to
   generate rule-based autonomous behavior that enables fully autonomous
   off-road driving. Overall, the intent of the paper is to evaluate
   approaches and technologies used in the two Grand Challenges as they
   contribute to the needs of autonomous cars on the road.}},
