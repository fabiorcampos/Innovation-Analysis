@article{ ISI:000304094000001,
Author = {Arogeti, Shai A. and Berman, Nadav},
Title = {{Path Following of Autonomous Vehicles in the Presence of Sliding Effects}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
Year = {{2012}},
Volume = {{61}},
Number = {{4}},
Pages = {{1481-1492}},
Month = {{MAY}},
Abstract = {{In this paper, a new controller for the path-following problem of a
   car-like autonomous vehicle is proposed. This controller is based on a
   new transformation that, for the case of roll with no slip, transfers
   the model to a chain form. However, in many practical cases, due to the
   strong effects of tire slip angles, a roll with no slip assumption is
   invalid. To cope with the presence of slip, a novel controller is
   proposed. The new method is based on a vehicle kinematical model, where
   the tire slip angles are taken into account. The control design method
   utilizes a peak-to-peak criterion and linear matrix inequality (LMI)
   tools to attenuate tire slip angle effects on the closed-loop
   performance.}},
