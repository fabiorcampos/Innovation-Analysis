@article{ ISI:000291001400005,
Author = {Sifuentes, E. and Casas, O. and Pallas-Areny, R.},
Title = {{Wireless Magnetic Sensor Node for Vehicle Detection With Optical Wake-Up}},
Journal = {{IEEE SENSORS JOURNAL}},
Year = {{2011}},
Volume = {{11}},
